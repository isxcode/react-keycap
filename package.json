{
		"name": "react-keycap",
		"version": "0.1.2",
		"description": "React rapid development component library.",
		"readme": "https://github.com/ispong/react-keycap/blob/master/README.md",
		"private": false,
		"license": "MIT",
		"keywords": [
				"react",
				"react-keycap",
				"keycap"
		],
		"homepage": "github.com/ispong/react-keycap/wiki",
		"repository": {
				"type": "git",
				"url": "https://github.com/ispong/react-keycap"
		},
		"main": "lib/index.js",
		"module": "es/index.js",
		"typings": "es/index.d.ts",
		"files": [
				"es",
				"lib"
		],
