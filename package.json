				"react": "^16.13.1",
				"react-dom": "^16.9.0",
				"react-scripts": "^3.4.3"
		},
		"devDependencies": {
				"@types/classnames": "^2.2.10",
				"classnames": "^2.2.6",
				"@babel/cli": "^7.11.6",
				"@babel/core": "^7.11.0",
				"@babel/preset-env": "^7.11.5",
				"cpx": "^1.5.0",
				"jest": "^26.1.0",
